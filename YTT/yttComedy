#!/bin/zsh

$HOME/.pyenv/shims/youtube-dl \
 --config-location $HOME/.config/youtube-dl/config/youtube-dl.conf \
 --playlist-items 1-20 \
 -o "$HOME/Video/channels/Comedy/%(uploader)s/%(upload_date)s - %(title)s.%(ext)s" > \
 /Volumes/fourtb/Video/channels/comedy/comedy_chan_log.txt 2>&1 \
 --batch-file $HOME/Archive/channels/comedy.txt

 echo "Who's laughing now sizzle chest."
